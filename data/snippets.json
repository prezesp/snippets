{
	"title": "[Snippets] Quick Start",
	"subtitle": "How to start with snippets...",
	"lang": "bash",
	"snippets": [
		{
			"title": "Clone repository",
			"fragments": [{
				"code": "$ git clone git@github.com:prezesp/snippets.git"
			}]
		},
		{
			"title": "Install node.js packages",
			"fragments": [{
				"code": "$ npm install"
			}]
		},
		{
			"title": "Run gulp",
			"fragments": [{
				"code": "$ gulp"
			}]
		},
		{
			"title": "Prepare your own course as json file",
			"fragments": [{
				"code": "$ touch data/my-course.json\n$ vi data/my-course.json"
			}]
		},
		{
			"title": "Update config.js and have fun",
			"fragments": [{
				"code": "$ vim src/config.js"
			}]
		}
	]
}
